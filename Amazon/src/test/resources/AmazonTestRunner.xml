<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Amazon app running on firefox Browser">
    <parameter name="usecloud" value="false"/>
    <parameter name="userName" value="your userName"/>
    <parameter name="accessKey" value="your key"/>
    <parameter name="os" value="Windows 10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="40"/>
    <parameter name="url" value="http://www.amazon.com"/>
    <test name="Home Page">
        <classes>
            <class name="search.ItemSearch"/>

        </classes>
    </test>
</suite>